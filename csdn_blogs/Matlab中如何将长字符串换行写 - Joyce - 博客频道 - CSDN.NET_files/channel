        var audios = document.getElementsByTagName('audio');
        $(document).on('click', '.js-music', function() {
            var music = ['1.mp3', '2.mp3', '3.mp3', '4.mp3'];
            var $this = $(this).index('.js-music'),
                audio = this.getElementsByTagName('audio');

            if (audio.length == 0) {
                for (var i = 0; i < audios.length; i++) {
                    audios[i].pause();
                    $('.music-item').find('.icon').removeClass('icon-puase');
                }
                var oadio = '<audio style=\"display:none\" src=\"' + music[$this] + '\" controls=\"controls\" autoplay=\"autoplay\" loop=\"loop\"></audio>';
                var odiv = '<div style=\"display:none\">' + oadio + '</div>';
                $(this).append(odiv);
                audio[0].play();
            } else if (audio[0].paused) {

                for (var i = 0; i < audios.length; i++) {
                    audios[i].pause();
                    $('.music-box').find(".btn").removeClass('puase');
                }
                audio[0].play();
            } else {
                audio[0].pause();
            }
            if ($(this).hasClass('puase')) {
                $(this).removeClass('puase');
            } else {
                $('.js-music').removeClass('puase');
                $(this).addClass('puase');
            }
        })

        var flag = true;
        window.onscroll = function() {
            var t = document.documentElement.scrollTop || document.body.scrollTop;


            $('.icon4').removeClass('active');
            $('.icon7').removeClass('active');
            $('.icon9').removeClass('active');
            if (t > 200 && t < 800 && flag) {
                $(".icon2").find('video')[0].play();
                flag = false;
            } else if (t < 200 || t > 800) {
                $(".icon2").find('video')[0].pause();
                flag = true;
            }

            if (t > 1700) {
                $('.icon4').addClass('active');
            }
            if (t > 4400) {
                $('.icon7').addClass('active');
            }
            if (t > 6000) {
                $('.icon9').addClass('active');
            }
            if (t > 6800) {
                $('.icon11').addClass('active');
            }

        }

        $('.log').on('mouseover', function() {
            $('.log').removeClass('active');
            $(this).addClass('active');
        })

        $('.hover').on('mouseover', function() {
            $(this).parent().addClass('active')
        })
        $('.hover').on('mouseout', function() {
            $(this).parent().removeClass('active');
        })

        $('.scrollTop1').on('click', function() {
            if(!$("html,body").is(":animated"))
            {$("html,body").animate({scrollTop:0},800)}
            return false;
        })

        var prevTop = 0,
            currTop = 0;
        $(window).scroll(function() {
            currTop = $(window).scrollTop();
            if (currTop < prevTop) { //判断小于则为向上滚动
                $(".sidebar").addClass("bottom");
                setTimeout(function() {
                    $(".sidebar").removeClass("bottom");
                }, 500)
            } else {
                $(".sidebar").addClass("top");
                setTimeout(function() {
                    $(".sidebar").removeClass("top");
                }, 500)
            }
            //prevTop = currTop; //IE下有BUG，所以用以下方式
            setTimeout(function() {
                prevTop = currTop
            }, 0);
        });
    })()
